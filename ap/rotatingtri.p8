//helpers
function mkpt(v)
	local pt={}
	pt.x = 32 + rnd(64)
	pt.y = 32 + rnd(64)
	pt.dx =0
	pt.dy =0
	pt.deg=atan2(pt.x-orig.x,pt.y-orig.y)
	pt.size= 3
	pt.c=rnd(3)+6
	pt.rad= pythag(pt.x-orig.x,pt.y-orig.y)
	return pt
end

function upt(pt)
  pt.dx = orig.x cos(pt.deg)
  pt.dy = orig.y sin(pt.deg)  
end

function ups(pt)
pt.x+=pt.dx
pt.y+=pt.dy
end

function dcirc(c)
	circfill(c.x,c.y,c.size,c.c)
end

function pythag(a,b)
  return sqrt(a^2+b^2)
end
