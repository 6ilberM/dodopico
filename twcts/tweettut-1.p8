pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
palette={0,2,8,9,7}
s=55
cls()
::█::
circfill(64+sin(t()/2)*s,64
+cos(t()/2)*s,7,7)

circfill(64+sin(-t()/2)*s/2,64
+cos(-t()/2)*s/2,3,7)

circfill(64+sin(t()/2)*s/4,64
+cos(t()/2)*s/4,1,7)
									
for i=1,250 do 
 x=rnd(128)
 y=rnd(128)
 for j=2,#palette do 
 	if pget(x,y)==palette[j]then
 		pset(x,y,palette[j-1])
 		end
 	end
end 
goto █
